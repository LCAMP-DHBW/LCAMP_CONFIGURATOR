    <!-- Link declaration v0.3 -->

    <xs:complexType name="link">
        <xs:annotation>
            <xs:documentation>
            A physical link with inertia, collision and visual properties. 
            A link must be a child of a component.
            Any number of links may exist in a component but they have to be grouped with an intermediate joint. 
            Each link needs a unique name.
            </xs:documentation>
