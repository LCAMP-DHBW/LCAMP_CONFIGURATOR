    <xs:complexType name="point_light">
        <xs:annotation>
            <xs:documentation>
                Used to define a light that gets emitted from a single point in all directions.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="distance" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Maximum range of the light in meters. Default is 0 (no limit).</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="decay" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) The amount the light dims along its distance, the default value of 2 gives a realistic effect. It's recommended to leave this as is.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"2"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="spot_light">
        <xs:annotation>
            <xs:documentation>
                Used to define a light that gets emitted from a single point in one direction, along a cone that increases in size the further from the light it gets.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="distance" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Maximum range of the light in meters. Default is 0 (no limit).</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="decay" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) The amount the light dims along its distance, the default value of 2 gives a realistic effect. It's recommended to leave this as is.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"2"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="angle" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Maximum angle of light dispersion from its direction in radians.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"1.047"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="penumbra" type="smtk_coefficient" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(coefficient) Percent of the spotlight cone that is attenuated due to penumbra.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0.0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="material">
        <xs:annotation>
            <xs:documentation>
                The material of the visual element.
                It overwrites the material of the mesh if provided.
            </xs:documentation>
        </xs:annotation>
        <xs:choice minOccurs="1" maxOccurs="1">
            <xs:element name="color" type="smtk_rgba" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[4]) The diffuse color of the material specified by the format: red green blue alpha , each in the range of [0,1].</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0 0 0 1", "labels":"r g b a"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="texture" type="smtk_guid" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) The texture of a material is specified by an asset GUID.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"extension":".png", "default":""}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    
    <xs:complexType name="surface">
        <xs:annotation>
            <xs:documentation>
                Collision surface parameters.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="bounciness" type="smtk_coefficient" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(coefficient) Bounciness coefficient of restitution, between [0..1]. When two objects collide, the total restitution is the multiplication of both values. Setting one to 0.0 will null out bounciness.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0.1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="friction" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Static friction value of the surface, between [0..1]. A good approximation would be that the friction between two surfaces A and B equals to max(A, B, A*B). A result of 0 would mean no friction and 1 maximum friction.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0.5"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="rolling_friction" type="smtk_float" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Torsional friction orthogonal to contact normal (useful to stop spheres rolling forever), between [0..1].</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0.0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="spinning_friction" type="smtk_float" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Torsional friction around the contact normal (useful for grasping), between [0..1].</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0.0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="collision_tag" type="smtk_collision_material_array" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str[]) Collision material properties. If no property is set, solid will be enabled by default.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"solid"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="collision_group" type="smtk_collision_group_array" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str[]) Collision group number. If not used, the object will collide with all objects.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"group_1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="motion_surface">
        <xs:annotation>
            <xs:documentation>
