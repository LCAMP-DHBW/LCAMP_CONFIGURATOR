                    <xs:appinfo>{"var_type":"constant" ,"default":"0 0 0 0 0 0 1", "labels":"x y z qx qy qz qw"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="parent" type="smtk_link" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) The name of the parent link.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":""}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="child" type="smtk_link" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) The name of the child link.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":""}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="axis" type="smtk_axis_array" minOccurs="1" maxOccurs="1">
