    <!-- Joint declaration v0.3 -->

    <xs:complexType name="joint">
        <xs:annotation>
            <xs:documentation>
            A joint connection between two links with kinematic and dynamic properties. 
            Notice that a joint will always be between two links. 
            The number of joints is equal to the number of links minus one.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="origin" type="smtk_transform" minOccurs="1" maxOccurs="1">
