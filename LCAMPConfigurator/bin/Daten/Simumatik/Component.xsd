            <xs:element name="motion_direction" type="smtk_vector3" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[3]) A normalized vector3 that indicates the direction for the movement.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0 0 0", "labels":"x y z"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="motion_speed" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
