    <!-- Choices types v0.3 -->
    <xs:simpleType name="smtk_mechanical_choice">
        <xs:annotation>
            <xs:documentation>
                velocity: The movement is specified by the velocity.

                position: The movement is specified by the position. It will move with the velocity limit.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="velocity" />
            <xs:enumeration value="position" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_sensor_choice">
        <xs:annotation>
            <xs:documentation>
                velocity: Shows the velocity of the joint.
                
                position: Shows the position of the joint.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="velocity" />
            <xs:enumeration value="position" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_collision_group_choice">
        <xs:annotation>
            <xs:documentation>
                A list of different collission groups that can be asigned to a collision object.
                Objects in different groups will not collide, while those without specific group will collide with all.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="group_1" />
            <xs:enumeration value="group_2" />
            <xs:enumeration value="group_3" />
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="smtk_collision_material_choice">
        <xs:annotation>
            <xs:documentation>
                A list of different materials properties that can be asigned to a collision object.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="all" />
            <xs:enumeration value="none" />
            <xs:enumeration value="solid" />
            <xs:enumeration value="liquid" />
            <xs:enumeration value="conductive" />
            <xs:enumeration value="reflective" />
            <xs:enumeration value="barcode" />
            <xs:enumeration value="rfid" />
	        <xs:enumeration value="magnet" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_font_choice">
        <xs:annotation>
            <xs:documentation>
                A list of different fonts that can be asigned to a text object.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="helvetiker" />
            <xs:enumeration value="optimer" />
            <xs:enumeration value="gentilis" />
            <xs:enumeration value="sevensegment" />
            <xs:enumeration value="ibmplexmono" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_sensor_technology_choice">
        <xs:annotation>
            <xs:documentation>
                A list of different sensing technologies that can be asigned to a sensing element.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="all" />
            <xs:enumeration value="none" />
            <xs:enumeration value="capacitive" />
            <xs:enumeration value="diffuse" />
            <xs:enumeration value="inductive" />
            <xs:enumeration value="photoelectric" />
            <xs:enumeration value="barcode" />
            <xs:enumeration value="rfid" />
            <xs:enumeration value="magnetic" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_shape_choice">
        <xs:annotation>
            <xs:documentation>
                matrix: Squared shape restricted to limits and containing equispaced rays.

                cone: All the rays goes from the centre to a circular projection shape restricted to limits and containing equispaced rays.

                pyramid: All the rays goes from the centre to a squared projection shape restricted to limits and containing equispaced rays.

                circular: All the rays goes from the center forming a circle with the length as radius.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="matrix" />
            <xs:enumeration value="cone" />
            <xs:enumeration value="pyramid" />
            <xs:enumeration value="circular" />
        </xs:restriction>
    </xs:simpleType>


