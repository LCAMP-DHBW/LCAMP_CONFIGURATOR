<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0.15">

    <!--Attribute type declaration -->

    <xs:simpleType name="smtk_name">
        <xs:annotation>
            <xs:documentation>Officially, element names in OEP can be any length and can consist of letters ( A-Z, a-z ), digits ( 0-9 ), and the underscore character ( _ ).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Za-z0-9_]+" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_var_name">
        <xs:annotation>
            <xs:documentation>Officially, variable names in OEP can be any length and can consist of letters ( A-Z, a-z ), digits ( 0-9 ), and the underscore character ( _ ). 
            The first character of the variable must be a letter or underscore (no digits allowed).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Za-z_][A-Za-z\d_]*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_guid">
        <xs:annotation>
            <xs:documentation>Restricts the input to match the official guid RegEx pattern.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="(\{){0,1}[0-9a-fA-F]{8}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{12}(\}){0,1}" />
        </xs:restriction>
    </xs:simpleType>

    <!-- Private type declaration -->

    <xs:simpleType name="smtk_private_string">
        <xs:restriction base="xs:string">
            <xs:pattern value="(.|\s)*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_noempty_string">
        <xs:restriction base="xs:string">
            <xs:pattern value="(.|\s)*\S(.|\s)*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_link">
        <xs:restriction base="xs:string">
            <xs:pattern value="(.|\s)*\S(.|\s)*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_bool">
        <xs:restriction base="xs:boolean">
            <xs:pattern value="(.|\s)*\S(.|\s)*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_int">
        <xs:restriction base="xs:integer">
            <xs:pattern value="(.|\s)*\S(.|\s)*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_float">
        <xs:restriction base="xs:float">
            <xs:pattern value="(.|\s)*\S(.|\s)*" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_axis">
        <xs:restriction base="xs:string">
            <xs:pattern value="(.|\s)*\S(.|\s)*" />
            <xs:enumeration value="fixed" />
            <xs:enumeration value="x" />
            <xs:enumeration value="y" />
            <xs:enumeration value="z" />
            <xs:enumeration value="rx" />
            <xs:enumeration value="ry" />
            <xs:enumeration value="rz" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_bytes">
        <xs:restriction base="xs:string" />
    </xs:simpleType>

    <!-- Coefficient type v0.3 -->
    <xs:simpleType name="smtk_private_coefficient">
        <xs:restriction base="xs:float">
            <xs:pattern value="(.|\s)*\S(.|\s)*" />
            <xs:minInclusive value="0" />
            <xs:maxInclusive value="1" />
        </xs:restriction>
    </xs:simpleType>

    <!-- Vector types v0.3 -->
    <xs:simpleType name="smtk_private_axis_array">
        <xs:list itemType="smtk_private_axis" />
    </xs:simpleType>

    <xs:simpleType name="smtk_private_vector">
        <xs:list itemType="smtk_private_float" />
    </xs:simpleType>

    <xs:simpleType name="smtk_private_string_array">
        <xs:list itemType="smtk_private_noempty_string" />
    </xs:simpleType>

    <xs:simpleType name="smtk_private_guid_array">
        <xs:list itemType="smtk_guid" />
    </xs:simpleType>

    <xs:simpleType name="smtk_private_coefficient_vector">
        <xs:list itemType="smtk_private_coefficient" />
    </xs:simpleType>

    <xs:simpleType name="smtk_private_vector2">
        <xs:restriction base="smtk_private_vector">
            <xs:length value="2" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_vector3">
        <xs:restriction base="smtk_private_vector">
            <xs:length value="3" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_rgba">
        <xs:restriction base="smtk_private_coefficient_vector">
            <xs:length value="4" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_vector6">
        <xs:restriction base="smtk_private_vector">
            <xs:length value="6" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_private_transform">
        <xs:restriction base="smtk_private_vector">
            <xs:length value="7" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="smtk_collision_group_array">
        <xs:list itemType="smtk_collision_group_choice" />
    </xs:simpleType>

    <xs:simpleType name="smtk_sensor_technology_array">
        <xs:list itemType="smtk_sensor_technology_choice" />
    </xs:simpleType>

    <xs:simpleType name="smtk_collision_material_array">
        <xs:list itemType="smtk_collision_material_choice" />
    </xs:simpleType>

    <!-- Public type declaration: this section just adds the public attribute -->

    <xs:complexType name="smtk_string">
        <xs:annotation>
            <xs:documentation>Simumatik String datatype. Can be empty</xs:documentation>
            <xs:appinfo>{"default":"{}"}</xs:appinfo>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="smtk_private_string">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="smtk_link">
        <xs:simpleContent>
            <xs:extension base="smtk_private_link">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="smtk_bool">
        <xs:annotation>
            <xs:documentation>Simumatik Boolean datatype.</xs:documentation>
            <xs:appinfo>{"default":"false"}</xs:appinfo>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="smtk_private_bool">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="smtk_int">
        <xs:annotation>
            <xs:documentation>Simumatik Integer datatype.</xs:documentation>
            <xs:appinfo>{"default":"0"}</xs:appinfo>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="smtk_private_int">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="smtk_float">
        <xs:simpleContent>
            <xs:extension base="smtk_private_float">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="smtk_axis">
        <xs:simpleContent>
            <xs:extension base="smtk_private_axis">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="smtk_bytes">
        <xs:simpleContent>
            <xs:extension base="smtk_private_bytes">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <!-- Coefficient type v0.3 -->
    <xs:complexType name="smtk_coefficient">
        <!-- Use to create a float value between 0 and 1 -->
        <xs:simpleContent>
            <xs:extension base="smtk_private_coefficient">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
