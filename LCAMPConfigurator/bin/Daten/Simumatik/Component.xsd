    <!-- smtk_var type v0.3 -->

    <xs:complexType name="smtk_var">
        <!-- Used to create user variables -->
        <xs:annotation>
            <xs:documentation>Behavior variable linked to the python script or de FMU in order to exchange information</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <!-- Use smtk_private_string instead of smtk_string to re-define public property -->
            <xs:extension base="smtk_private_string">
                <xs:attribute name="name" type="smtk_var_name" use="required" />
                <xs:attribute name="data_type" use="required">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="smtk_bool" />
			                <xs:enumeration value="smtk_bytes" />
                            <xs:enumeration value="smtk_int" />
                            <xs:enumeration value="smtk_float" />
                            <xs:enumeration value="smtk_string" />
                            <xs:enumeration value="smtk_vector" />
                            <xs:enumeration value="smtk_vector2" />
                            <xs:enumeration value="smtk_vector3" />
                            <xs:enumeration value="smtk_rgba" />
                            <xs:enumeration value="smtk_vector6" />
                            <xs:enumeration value="smtk_coefficient" />
                            <xs:enumeration value="smtk_transform" />
                            <xs:enumeration value="smtk_mechanical_choice" />
                            <xs:enumeration value="smtk_camera_format_choice" />
                            <xs:enumeration value="smtk_camera_publish_choice" />
                            <xs:enumeration value="smtk_sensor_technology_array" />
                            <xs:enumeration value="smtk_comm_driver_choice" />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="type" use="required">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="input" />
                            <xs:enumeration value="output" />
                            <xs:enumeration value="constant" />
                            <xs:enumeration value="parameter" />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <!-- Re-define so it's easier for the app to parse -->
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <!-- First level complex elements declaration -->

    <!-- Symbol declaration v0.3 -->

    <xs:complexType name="symbol">
        <xs:annotation>
            <xs:documentation>
                The symbol represents the 2D view of the component. 
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="image" type="smtk_guid" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Image asset GUID.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"extension":".jpg .png .svg", "default":""}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="size" type="smtk_vector2" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(int[2]) Size of the image in pixels.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant", "default":"100 100", "labels":"width height"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" type="smtk_name" use="required" />
    </xs:complexType>

    <!-- Link starts v0.3 -->

    <xs:complexType name="inertial">
        <xs:annotation>
            <xs:documentation>
                The inertial properties of the link.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="origin" type="smtk_transform" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        [NOT IMPLEMENTED] (float[7]) The pose of the inertial reference frame, relative to the link reference frame.
                    
                        The origin of the inertial reference frame needs to be at the center of gravity. 

                        The axes of the inertial reference frame do not need to be aligned with the principal axes of the inertia: x y z qx qy qz qw    
                    </xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0 0 0 0 0 0 1", "labels":"x y z qx qy qz qw"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="mass" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) The mass of the link. It is recommended to limit the value betwen 0.05-1.0</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="inertia" type="smtk_vector6" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        (float[6]) The 3x3 rotational inertia matrix, represented in the inertia frame. The inertia matrix is used only when the link does not have a collision shape.
                        In that case the inertia is automatically calculated by the physics engine.
                        Because the rotational inertia matrix is symmetric, only 6 above-diagonal elements of this matrix are specified here, in the form: ixx, ixy, ixz, iyy, iyz, izz.
                        Note that for limitations in the physics engine, only the diagonal elements of the matrix (ixx, iyy and izz) will be used.
                    </xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"1 0 0 1 0 1", "labels":"ixx, ixy, ixz, iyy, iyz, izz"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="disable_calculate_local_inertia" type="smtk_bool" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) This flag can be used to disable calculating inertia vector from the collision shapes and instead use the value in the vector.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"false"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Display starts v0.3 -->

    <xs:complexType name="display">
        <xs:annotation>
            <xs:documentation>
                This element represents a display that can show content. 
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="origin" type="smtk_transform" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        (float[7]) The pose of the inertial reference frame, relative to the link reference frame.
                    </xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0 0 0 0 0 0 1", "labels":"x y z qx qy qz qw"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="size" type="smtk_vector2" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[2]) The size of the display in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"1 1", "labels":"width height"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="source" type="smtk_string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) The source for the display, URL direction.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"None"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" type="smtk_name" use="required" />
    </xs:complexType>

    <xs:complexType name="mesh">
        <xs:annotation>
            <xs:documentation>
                A trimesh element specified by an asset. If the file includes materials, they are applied. 

                The supported format is .glb.

                By default, for mesh used in collision, each component of the glb file is simplified with a convex shape. It is adviced to use a preprocessing tool to separate the glb into convex shapes previously (e.g: V-HACD).
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="model" type="smtk_guid" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Model asset GUID.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"extension":".glb .obj", "default":""}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="scale" type="smtk_vector3" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[3]) Scale the mesh's axis-aligned-bounding-box.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"1 1 1", "labels":"x y z"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="as_convex_hull" type="smtk_bool" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) If this optional variable is set to true, the convex hull is computed for the whole model instead of each part separately. </xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"true"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="as_triangle_mesh" type="smtk_bool" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) If this optional variable is set to true, the model is used as is. It works only for static links.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"true"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="simplify" type="smtk_bool" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) If this optional variable is set to true, a convex hull will be automatically created for the model.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"true"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="text">
        <xs:annotation>
            <xs:documentation>
                A text geometry allows displaying text in the component. Select a font and use the different attributes to adjust its appearance.
                The value attribute is used to set the string to be displayed.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="value" type="smtk_string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Text to be displayed.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"text"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="font" type="smtk_font_choice" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) The font used for the text. </xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"helvetiker"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="height" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Size of the text.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="thickness" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Thickness to extrude text.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"0.5"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="bold" type="smtk_bool" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) Allows the text to be displayed regular (if false) or bold (if true).</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"true"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="box2d">
        <xs:annotation>
            <xs:documentation>
                The 2 dimensional box is built on the XY axes, facing Z axis. The origin of the box2d is in its center.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="size" type="smtk_vector2" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[2]) Specify the width (X) and height (Y) lengths of the box2d in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"1 1", "labels":"x y"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
	    <xs:element name="scale" type="smtk_vector3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[3]) Scale applied to the geometry with the given attributes.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"1 1 1", "labels":"x y z"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="box">
        <xs:annotation>
            <xs:documentation>
                The origin of the box is in its center.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="size" type="smtk_vector3" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[3]) Specify the three side lengths of the box in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"1 1 1", "labels":"x y z"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
	    <xs:element name="scale" type="smtk_vector3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[3]) Scale applied to the geometry with the given attributes.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"1 1 1", "labels":"x y z"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="cylinder">
        <xs:annotation>
            <xs:documentation>
                The origin of the cylinder is in its center. The cylinder is built around the Y axis.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="radius" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Specify the radius in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="length" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Specify the length in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant", "default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
	    <xs:element name="scale" type="smtk_vector3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[3]) Scale applied to the geometry with the given attributes.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"1 1 1", "labels":"x y z"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="sphere">
        <xs:annotation>
            <xs:documentation>
                The origin of the sphere is in its center.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="radius" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Specify the radius in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant", "default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
	    <xs:element name="scale" type="smtk_vector3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[3]) Scale applied to the geometry with the given attributes.</xs:documentation>
                    <xs:appinfo>{"var_type":"input", "default":"1 1 1", "labels":"x y z"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="capsule">
        <xs:annotation>
            <xs:documentation>
                The origin of the capsule is in its center. The capsule is built around the Y axis.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="radius" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Specify the radius in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"1"}</xs:appinfo>
