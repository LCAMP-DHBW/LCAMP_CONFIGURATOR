            <xs:element name="samples" type="smtk_int" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(integer) Number of rays casted.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="limits" type="smtk_vector2" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[2]) Dispersion of the rays in meters.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0 0", "labels":"min max"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
