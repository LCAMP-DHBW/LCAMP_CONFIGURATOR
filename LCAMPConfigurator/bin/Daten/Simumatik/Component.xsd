    <!-- Drivers declaration v0.3 -->
    <xs:complexType name="communication_driver">
        <xs:annotation>
            <xs:documentation>
                Models the communication with the gateway in order to connect to different software.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="type" type="smtk_comm_driver_choice" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) The type of driver. Each driver has its required setup data.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"opcua_client"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="enable" type="smtk_bool" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) Enable or disable the driver.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"false"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="setup_data" type="smtk_string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Setup for the driver in JSON. The message depends on the driver used.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"{}"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="update_input" type="smtk_string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Update the inputs to the driver from the system in JSON. The structure of the message is defined by the variables.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"{}"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="smtk_string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) The status of the driver (connecting, connected, standby, running, error).</xs:documentation>
                    <xs:appinfo>{"var_type":"output" ,"default":"none"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="info" type="smtk_string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Debug messages from the driver.</xs:documentation>
                    <xs:appinfo>{"var_type":"output" ,"default":"none"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="update_output" type="smtk_string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Update the outputs from the driver to the system in JSON. The structure of the message is defined by the variables.</xs:documentation>
                    <xs:appinfo>{"var_type":"output" ,"default":"{}"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" type="smtk_name" use="required" />
    </xs:complexType>

