    <!-- Joint starts v0.3 -->

    <xs:complexType name="dynamics">
        <xs:annotation>
            <xs:documentation>
                The physical properties of the joint when the spring feature wants to be enabled.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="spring_damping" type="smtk_coefficient_vector" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(coefficient[]) Viscous damping coefficient of the spring, from [0.0, 1.0].</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"0.001"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="spring_reference" type="smtk_vector" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[]) The spring position reference in m or rad.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="spring_stiffness" type="smtk_vector" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[]) The spring stiffness value in m or rad.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"100"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="limits">
        <xs:annotation>
            <xs:documentation>
                The limits of the joint. In order to fix the joint, the limits must be equal (i.e. 0,0). In order to not use limits, the lower must be greater than the upper and be located in the first or second quadrant positive or negative (i.e. -pi to pi)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lower" type="smtk_vector" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[]) Lower joint limit. In radians for revolute joints and length for prismatic joints in m or rad.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="upper" type="smtk_vector" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[]) Upper joint limit. In radians for revolute joints and length for prismatic joints in m or rad.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="effort" type="smtk_vector" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[]) Maximum effort for the joint in N or N*m.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"1000"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="velocity" type="smtk_vector" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float[]) Maximum velocity for the joint in m/s or rad/s.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="joint_sensor">
        <xs:annotation>
            <xs:documentation>
                The modelling of a sensor in the joint.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="type" type="smtk_sensor_choice" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) defines the sensor for the joint. This can be: position, velocity (TODO: effort).</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"position"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="axis" type="smtk_axis" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(axis) indicates the axis to be sensed: x, y, z, rx, ry, rz.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"x"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="update_rate" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) The frequency at which the sensor data is generated automatically. If set to 0 the sensor will not automatically update the data.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"default":"0.1"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="enable" type="smtk_bool" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) Enable or disable the joint sensor.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"true"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="value" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) value for the motor sensing.</xs:documentation>
                    <xs:appinfo>{"var_type":"output" ,"default":"0.0"}</xs:appinfo>
                </xs:annotation>
