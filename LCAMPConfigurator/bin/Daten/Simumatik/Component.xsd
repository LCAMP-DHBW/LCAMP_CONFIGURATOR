            <xs:element name="joint_motor" type="joint_motor" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="override_iterations" type="smtk_int" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(int) It overrides the default constraint solver iteration value.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"50"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="disable_collisions_between_linked_bodies" type="smtk_bool" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(bool) It disables collisions between linked bodies by default (if True), for more stability.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"true"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" type="smtk_name" use="required" />
    </xs:complexType>

    <xs:group name="joint_link">
        <xs:sequence>
