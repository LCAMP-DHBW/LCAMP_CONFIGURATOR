        <xs:attribute name="name" type="smtk_name" use="required" />
    </xs:complexType>

    <!-- Behavior declaration v0.3 -->

    <xs:complexType name="behavior">
        <xs:annotation>
            <xs:documentation>
                The behavior object represents a black box which will include some logic, in form of a python script or [NOT IMPLEMENTED] fmu function, that can be used to model the component behavior.
                The behavior logic will be interfaced using variables that can be connected to other elements. 

                Variables will be defined inside the 'variable' element by their name, data type (smtk_...), type (input, output, parameter or constant) and optionally public or not.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice minOccurs="1" maxOccurs="1">
                <xs:element name="script" type="smtk_guid" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>(str) Python script asset GUID.</xs:documentation>
                        <xs:appinfo>{"var_type":"constant" ,"extension":".py", "default":""}</xs:appinfo>
                    </xs:annotation>
                </xs:element>
                <!--xs:element name="fmu" type="smtk_guid" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>(str) FMU asset GUID.</xs:documentation>
                        <xs:appinfo>{"var_type":"constant" ,"extension":".fmu", "default":""}</xs:appinfo>
                    </xs:annotation>
                </xs:element-->
            </xs:choice>
            <xs:element name="library" type="smtk_guid" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(str) Python library asset GUID. This library is loaded during the build process to allow additional methods and classes.</xs:documentation>
                    <xs:appinfo>{"var_type":"constant" ,"extension":".py", "default":""}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="step_time" type="smtk_float" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>(float) Step time to force the execution of the behavior logic periodically in seconds. If it is 0, it is executed when any input changes.</xs:documentation>
                    <xs:appinfo>{"var_type":"input" ,"default":"0.0"}</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="variable" type="smtk_var" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="name" type="smtk_name" use="required" />
    </xs:complexType>

