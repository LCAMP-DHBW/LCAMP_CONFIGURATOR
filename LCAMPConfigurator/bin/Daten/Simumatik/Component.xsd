            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="smtk_vector6">
        <xs:simpleContent>
            <xs:extension base="smtk_private_vector6">
                <xs:attribute name="public" type="xs:boolean" default="false"></xs:attribute>
